{
	"manifest_version": 2,
	"version": "3.2",
	"name": "elementary OS Theme for Google Chrome",
	"description": "An elementary OS -like Theme for Google Chrome.",
	"icons": {
		"16": "icon.png",
		"32": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"theme": {
		"images": {
			"theme_frame": "theme_frame.png",
			"theme_toolbar": "theme_toolbar.png"
		},
		"colors": {
			"frame": [201, 201, 201],
			"ntp_text": [51, 51, 51],
			"ntp_link": [0, 136, 204],
			"ntp_link_underline": [0, 136, 204],
			"ntp_section_text": [51, 51, 51],
			"ntp_section_link": [0, 136, 204],
			"ntp_section_link_underline": [0, 136, 204],
			"tab_text": [77,77,77],
			"tab_background_text": [77,77,77],
			"bookmark_text": [77,77,77],
			"button_background": [0,0,0,0]
		},
		"tints":{
			"frame": [-1,-1,-1],
			"frame_inactive": [-1,-1,-1],
			"frame_incognito": [-1,-1,-1],
			"frame_incognito_inactive": [-1,-1,-1],
			"background_tab": [-1,-1,0.7]
		}
	}
}
