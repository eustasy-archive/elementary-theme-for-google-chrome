{
	"manifest_version": 2,
	"version": "3.2",
	"name": "Freya Theme (Classic Colors)",
	"description": "An elementary Freya -like Theme for Google Chrome with more classic Chrome colors.",
	"icons": {
		"16": "icon.png",
		"32": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"theme": {
		"images": {
			"theme_frame": "theme_frame.png",
			"theme_toolbar": "theme_toolbar.png"
		},
		"tints":{
			"frame": [-1,-1,-1],
			"frame_inactive": [-1,-1,-1],
			"frame_incognito": [-1,-1,-1],
			"frame_incognito_inactive": [-1,-1,-1],
			"background_tab": [-1,-1,0.7]
		}
	}
}