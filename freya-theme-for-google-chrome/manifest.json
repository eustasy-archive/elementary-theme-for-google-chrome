{
	"manifest_version": 2,
	"version": "3.1",
	"name": "Freya Theme for Google Chrome",
	"description": "An elementary Freya -like Theme for Google Chrome.",
	"icons": {
		"16": "icon.png",
		"32": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"theme": {
		"images": {
			"theme_frame": "theme_frame.png",
			"theme_toolbar": "theme_toolbar.png"
		},
		"colors": {
			"frame": [200,200,200],
			"frame_inactive": [200,200,200],
			"frame_incognito": [200,200,200],
			"frame_incognito_inactive": [200,200,200],
			"toolbar": [217,217,217],
			"tab_text": [77,77,77],
			"tab_background_text": [77,77,77],
			"bookmark_text": [77,77,77],
			"ntp_background": [255,255,255],
			"ntp_text": [77,77,77],
			"button_background": [0,0,0,0]
		},
		"properties": {
			"ntp_background_alignment": "bottom",
			"ntp_background_repeat": "no-repeat"
		},
		"tints":{
			"buttons": [0,0,0.36],
			"frame": [-1,-1,-1],
			"frame_inactive": [-1,-1,-1],
			"frame_incognito": [-1,-1,-1],
			"frame_incognito_inactive": [-1,-1,-1],
			"background_tab": [-1,-1,0.7]
		}
	}
}